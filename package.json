{
  "name": "naive-definitions",
  "displayName": "Naive Definitions",
  "description": "Naive 'Go to Definition' for JavaScript",
  "license": "MIT",
  "version": "0.0.2",
  "publisher": "FallenMax",
  "repository": {
    "type": "git",
    "url": "https://github.com/fallenmax/naive-definitions"
  },
  "bugs": {
    "url": "https://github.com/fallenmax/naive-definitions/issues"
  },
  "keywords": ["javascript"],
  "engines": {
    "vscode": "^1.0.0"
  },
  "categories": ["Languages"],
  "activationEvents": ["onLanguage:javascript", "onLanguage:javascriptreact"],
  "main": "./out/src/extension",
  "scripts": {
    "test": "jest",
    "vscode:prepublish": "tsc -p ./",
    "compile": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install"
  },
  "devDependencies": {
    "@types/jest": "^21.1.8",
    "@types/node": "^8.5.1",
    "jest": "^21.2.1",
    "ts-jest": "^21.2.4",
    "typescript": "^2.6.2",
    "vscode": "^1.1.10"
  },
  "dependencies": {},
  "jest": {
    "moduleDirectories": ["node_modules"],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": ["ts", "tsx", "js", "jsx", "json", "node"]
  }
}
