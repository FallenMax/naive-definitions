{
  "name": "naive-definitions",
  "displayName": "Naive Definitions",
  "description": "Naive 'Go to Definition' for JavaScript",
  "license": "MIT",
  "version": "0.0.6",
  "publisher": "FallenMax",
  "repository": {
    "type": "git",
    "url": "https://github.com/fallenmax/naive-definitions"
  },
  "bugs": {
    "url": "https://github.com/fallenmax/naive-definitions/issues"
  },
  "keywords": [
    "javascript"
  ],
  "engines": {
    "vscode": "^1.0.0"
  },
  "categories": [
    "Languages"
  ],
  "activationEvents": [
    "onLanguage:javascript",
    "onLanguage:javascriptreact",
    "onLanguage:typescript",
    "onLanguage:typescriptreact",
    "onLanguage:vue"
  ],
  "main": "./out/src/extension",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "vscode:prepublish": "tsc -p ./ && npm run test",
    "compile": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install"
  },
  "devDependencies": {
    "@types/jest": "^24.0.13",
    "@types/node": "^12.0.3",
    "jest": "^24.8.0",
    "ts-jest": "^24.0.2",
    "typescript": "^3.4.5",
    "vscode": "^1.0.0"
  },
  "dependencies": {},
  "jest": {
    "moduleDirectories": [
      "node_modules"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
