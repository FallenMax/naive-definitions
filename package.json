{
  "name": "naive-definitions",
  "displayName": "Naive Definitions",
  "description": "Naive 'Go to Definition' for JavaScript",
  "license": "MIT",
  "version": "0.0.1",
  "publisher": "FallenMax",
  "repository": {
    "type": "git",
    "url": "https://github.com/fallenmax/naive-definitions"
  },
  "bugs": {
    "url": "https://github.com/fallenmax/naive-definitions/issues"
  },
  "keywords": ["javascript"],
  "engines": {
    "vscode": "^1.0.0"
  },
  "categories": ["Languages"],
  "activationEvents": ["onLanguage:javascript"],
  "main": "./out/src/extension",
  "contributes": {
    "commands": [
      {
        "command": "editor.gotoNaiveDefinitions",
        "title": "Go to Naive Definitions"
      }
    ],
    "configuration": {
      "properties": {
        "naivedefinitions.integrateWithGoToDefinition": {
          "type": "boolean",
          "default": true,
          "description": "Integrate with 'Go to Definition'"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "tsc -p ./",
    "compile": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install"
  },
  "devDependencies": {
    "@types/node": "^8.5.1",
    "typescript": "^2.6.2",
    "vscode": "^1.1.10"
  },
  "dependencies": {}
}
